SELECT

-- PersonId to match with all View's // tblCustody
    tC.serviceTblPerson AS TblPerson_Id,
    tC.Occupation AS TblCustody_Occupation,
    tC.Employment AS TblCustody_Employment,
    tC.Remark AS TblCustody_Remark

FROM PeopleMeta_{{ ConsumerAcronym }}.tblCustody tC

WHERE tC.EntityRemove IS NULL