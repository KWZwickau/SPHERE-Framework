SELECT

-- PersonId to match with all View's // tblStudent
    tS.serviceTblPerson AS TblPerson_Id,
    tS.Id AS TblStudent_Id,
    tS.Identifier AS TblStudent_Identifier,
    DATE_FORMAT( tS.SchoolAttendanceStartDate, '%d.%m.%Y' ) AS TblStudent_SchoolAttendanceStartDate,
    IF (tS.HasMigrationBackground, 'Ja', 'Nein') AS TblStudent_HasMigrationBackground,
    tS.MigrationBackground AS TblStudent_MigrationBackground,
    IF (tS.IsInPreparationDivisionForMigrants, 'Ja', 'Nein') AS TblStudent_IsInPreparationDivisionForMigrants

FROM {{ ConsumerAcronym }}_PeopleMeta.tblStudent tS

WHERE tS.EntityRemove is Null