SELECT

-- PersonId to match with all View's // tblClub
    tC.serviceTblPerson AS TblPerson_Id,
    tC.Identifier AS TblClub_Identifier,
    DATE_FORMAT( tC.EntryDate, '%d.%m.%Y' ) AS TblClub_EntryDate,
    DATE_FORMAT( tC.ExitDate, '%d.%m.%Y' ) AS TblClub_ExitDate,
    tC.Remark AS TblClub_Remark

FROM PeopleMeta_{{ ConsumerAcronym }}.tblClub tC

WHERE tC.EntityRemove IS NULL